{
  "name": "auth",
  "requests": [
    {
      "name": "signin",
      "url": "{{URL}}:{{PORT}}/auth/signin",
      "method": "POST",
      "params": [],
      "headers": [
        {
          "enabled": true,
          "data": [
            "cache-control",
            "no-cache"
          ]
        },
        {
          "enabled": true,
          "data": [
            "user-agent",
            "ATAC/v0.19.0"
          ]
        },
        {
          "enabled": true,
          "data": [
            "accept",
            "*/*"
          ]
        },
        {
          "enabled": true,
          "data": [
            "accept-encoding",
            "gzip, deflate, br"
          ]
        },
        {
          "enabled": true,
          "data": [
            "connection",
            "keep-alive"
          ]
        },
        {
          "enabled": true,
          "data": [
            "content-type",
            "application/json"
          ]
        }
      ],
      "body": {
        "json": "{\n    \"username\": \"test\",\n    \"password\": \"1234\"\n}"
      },
      "auth": "no_auth",
      "scripts": {
        "pre_request_script": null,
        "post_request_script": null
      },
      "settings": {
        "use_config_proxy": true,
        "allow_redirects": true,
        "store_received_cookies": true,
        "pretty_print_response_content": true,
        "accept_invalid_certs": false,
        "accept_invalid_hostnames": false
      }
    }
  ]
}